# 10-12


F1 <- c(3.1,3.14,3.08,3.2,2.84)
F2 <- c(2.76,2.88,2.88,3.08,2.93)
F3 <- c(3.19,3.13,3.45,3.34,3.00)
F4 <- c(2.84,2.72,2.61,2.65,2.61)
dat <- tibble (
  F1,F2,F3,F4
)
s_total <- sd(c(dat$F1,dat$F2,dat$F3,dat$F4))
MS_total <- s_total^2
df_total <- sum(c(length(dat$F1),
                  length(dat$F2),
                  length(dat$F3),
                  length(dat$F4))) - 1
SS_total <- MS_total * df_total
MS_total
df_total
SS_total





